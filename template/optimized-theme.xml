    <!-- Critical CSS for above-the-fold content -->
    <style>
      /* Critical CSS for faster loading */
      body { 
        font-family: 'Noto Sans', system-ui, -apple-system, sans-serif; 
        margin: 0; 
        line-height: 1.6; 
        color: #656565; 
      }
      .skip-link {
        position: absolute;
        top: -40px;
        left: 6px;
        background: #f12020;
        color: white;
        padding: 8px;
        text-decoration: none;
        z-index: 1001;
        border-radius: 4px;
      }
      .skip-link:focus {
        top: 6px;
      }
      #outer-wrapper { 
        max-width: 1200px; 
        margin: 0 auto; 
        background: #fff; 
        box-shadow: 0 0 5px rgba(0,0,0,.1); 
      }
      .loading { 
        text-align: center; 
        padding: 20px; 
        color: #999; 
      }
      .loading::after {
        content: '';
        display: inline-block;
        width: 20px;
        height: 20px;
        border: 2px solid #f3f3f3;
        border-top: 2px solid #f12020;
        border-radius: 50%;
        animation: spin 1s linear infinite;
        margin-left: 10px;
      }
      @keyframes spin {
        0% { transform: rotate(0deg); }
        100% { transform: rotate(360deg); }
      }
      .sr-only {
        position: absolute;
        width: 1px;
        height: 1px;
        padding: 0;
        margin: -1px;
        overflow: hidden;
        clip: rect(0, 0, 0, 0);
        white-space: nowrap;
        border: 0;
      }
    </style>
    
